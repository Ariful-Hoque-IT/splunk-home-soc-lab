<dashboard version="1.1">
  <label>Home SOC Lab Dashboard</label>
  <row>
    <panel>
      <title>Firewall Blocked Connections (Last 24h)</title>
      <table>
        <search>
          <query>index=main sourcetype="pfirewall.log" action=BLOCK direction=Outbound | stats count by dest_ip dest_port | sort -count</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
      </table>
    </panel>
    <panel>
      <title>Recent Process Creations (4688)</title>
      <table>
        <search>
          <query>index=main EventCode=4688 | table _time New_Process_Name Process_Command_Line | sort -_time</query>
          <earliest>-1h@h</earliest>
          <latest>now</latest>
        </search>
      </table>
    </panel>
  </row>
</dashboard>
